ratings=../dataset/FilmTrust/ratings.txt
ratings.setup=-columns 0 1 2 -threshold -1
recommender=UserKNN
evaluation=-testSet ../dataset/FilmTrust/ratings.txt -p on --rand-seed 1 --test-view all #-testset
item.ranking=off -topN -1 -ignore -1
similarity=PCC
num.shrinkage=30
num.neighbors=50
output.setup=on -dir ../results/